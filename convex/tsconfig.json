{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ES2021",
            "dom"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "_generated"
    ]
}